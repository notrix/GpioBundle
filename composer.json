{
    "name": "notrix/gpio-bundle",
    "license": "MIT",
    "type": "symfony-bundle",
    "description": "Raspberry PI gpio bundle",
    "keywords": ["raspberry", "pi", "gpio", "pir"],
    "authors": [
        {
            "name" : "Vaidas LaÅ¾auskas",
            "email" : "vaidas@notrix.lt",
            "homepage": "http://notrix.lt"
        }
    ],
    "autoload": {
